{
  "name": "tableau-visual-regression",
  "version": "0.1.0",
  "description": "tableau.com visual regression testing",
  "repository": "https://github.com/tableau-mkt/tableau_visual_regression",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "openReport": "node app.js --openReport",
    "approve": "node app.js --approve",
    "reference": "node app.js --reference",
    "single": "node lib/single.js",
    "migration": "node lib/generic.js --projectID migration --scenarios migration",
    "demo": "node lib/generic.js --projectID demo --scenarios demo",
    "header-footer": "node lib/generic.js --projectID header-footer --scenarios header-footer",
    "paragraphs": "node lib/generic.js --projectID paragraphs --scenarios paragraphs"
  },
  "keywords": [
    "visual_regression",
    "testing",
    "backstopjs"
  ],
  "author": "Leslie Doherty <ldoherty@tableau.com>",
  "license": "ISC",
  "_moduleAliases": {
    "@root": ".",
    "@config": "backstop_config",
    "@scenarios": "backstop_config/scenarios",
    "@lib": "lib",
    "@data": "backstop_data",
    "@html_report": "backstop_data/html_report",
    "@ci_report": "backstop_data/ci_report",
    "@engine_scripts": "backstop_data/engine_scripts",
    "@bitmap_reference": "backstop_data/bitmap_reference",
    "@bitmap_test": "backstop_data/bitmap_test"
  },
  "jest": {
    "moduleNameMapper": {
      "^@lib/(.*)$": "<rootDir>/lib/$1"
    }
  },
  "dependencies": {
    "backstopjs": "^5.0.1",
    "date-format": "^2.1.0",
    "jest": "^26.4.2",
    "lodash.mergewith": "^4.6.2",
    "module-alias": "^2.2.2",
    "selenium-webdriver": "^4.0.0-alpha.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "standard": "^14.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "storage/*"
    ]
  },
  "standard": {
    "ignore": [
      "node_modules/**"
    ]
  }
}
